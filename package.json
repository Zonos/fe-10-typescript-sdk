{
  "name": "@zonos/zonos-typescript-sdk",
  "version": "0.0.1",
  "private": false,
  "dependencies": {
    "graphql": "^16.6.0",
    "graphql-request": "^4.3.0",
    "graphql-tag": "^2.12.6"
  },
  "engines": {
    "node": "18"
  },
  "publishConfig": {
    "@zonos:registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@graphql-codegen/add": "^3.2.3",
    "@graphql-codegen/cli": "^2.14.1",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.8.8",
    "@graphql-codegen/typescript-graphql-request": "^4.5.9",
    "@graphql-codegen/typescript-operations": "^2.5.7",
    "@rollup/plugin-buble": "^1.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-deprecation": "^1.3.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "glob": "^9.3.2",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "rollup": "^3.20.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-sizes": "^1.0.5",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-toolbelt": "^9.6.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.4.6"
  },
  "peerDependencies": {},
  "scripts": {
    "lint": "eslint src --ext .ts",
    "ci": "yarn tsc && yarn lint && yarn test:prod",
    "ci:concurrent": "concurrently 'yarn tsc' 'yarn lint' 'yarn test:prod' --group --raw --success='all'",
    "prebuild:ci": "yarn prebuild",
    "prebuild:dts": "echo 'building dts'",
    "prebuild:prep:publish": "echo 'building for publishing'",
    "prebuild": "echo 'start cleaning up' && rm -rf dist && echo 'done cleaning up'",
    "build:ci": "ts-node build-utils/build.ts && yarn build:dts",
    "build:dts": "ttsc -p ./tsconfig.emitDeclarationOnly.json",
    "build:prep:publish": "ts-node build-utils/preparePublish.ts",
    "build": "yarn && yarn codegen && yarn ci:concurrent && ts-node build-utils/build.ts && yarn build:dts",
    "postbuild:ci": "yarn postbuild",
    "postbuild:dts": "echo 'done build dts'",
    "postbuild:prep:publish": "echo 'done building for publishing'",
    "postbuild": "yarn build:prep:publish",
    "test": "jest --watchAll",
    "tsc": "tsc --noEmit",
    "test:prod": "jest",
    "version:patch": "npm version patch -m 'Bump version %s' && yarn push:tags",
    "version:minor": "npm version minor -m 'Bump version %s' && yarn push:tags",
    "version:major": "npm version major -m 'Bump version %s' && yarn push:tags",
    "push:tags": "git push origin $(git rev-parse --abbrev-ref HEAD) --follow-tags",
    "codegen": "graphql-codegen -r dotenv/config -c codegen/codegen.ts"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
