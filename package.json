{
  "name": "@zonos/typescript-sdk",
  "version": "0.1.2",
  "private": false,
  "peerDependencies": {
    "graphql": "^16.x",
    "graphql-request": "^4.x",
    "graphql-tag": "^2.x"
  },
  "engines": {
    "node": "18"
  },
  "publishConfig": {
    "@zonos:registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "graphql": "^16.8.1",
    "graphql-request": "^4.3.0",
    "graphql-tag": "^2.12.6",
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.1",
    "@graphql-codegen/add": "^3.2.3",
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/introspection": "^2.2.3",
    "@graphql-codegen/typescript": "3.0.4",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^2.5.13",
    "@rollup/plugin-buble": "^1.0.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.28",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "concurrently": "^7.6.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-deprecation": "^1.5.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-typescript-custom-sort-keys": "^1.0.5",
    "eslint-plugin-vitest": "^0.2.8",
    "glob": "^9.3.5",
    "prettier": "^2.8.8",
    "rollup": "^3.29.4",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-sizes": "^1.0.6",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "ts-toolbelt": "^9.6.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tslib": "^2.6.2",
    "typescript": "5.4.3",
    "typescript-transform-paths": "^3.4.7",
    "vite": "^5.2.7",
    "vitest": "^1.4.0"
  },
  "scripts": {
    "lint": "eslint src --ext .ts",
    "ci": "pnpm tsc && pnpm lint && pnpm test:prod",
    "ci:concurrent": "concurrently 'pnpm tsc' 'pnpm lint' 'pnpm test:prod' --group --raw --success='all'",
    "prebuild:ci": "pnpm prebuild",
    "prebuild:dts": "echo 'building dts'",
    "prebuild:prep:publish": "echo 'building for publishing'",
    "prebuild": "echo 'start cleaning up' && rm -rf dist && echo 'done cleaning up'",
    "build:ci": "ts-node build-utils/build.ts && pnpm build:dts",
    "build:dts": "tspc -p ./tsconfig.emitDeclarationOnly.json",
    "build:prep:publish": "ts-node build-utils/preparePublish.ts",
    "build": "pnpm i && pnpm codegen && pnpm ci:concurrent && pnpm build:ci && pnpm build:dts && pnpm postbuild",
    "postbuild:ci": "pnpm postbuild",
    "postbuild:dts": "echo 'done build dts'",
    "postbuild:prep:publish": "echo 'done building for publishing'",
    "postbuild": "pnpm build:prep:publish",
    "test": "vitest --no-watch",
    "tsc": "tsc --noEmit",
    "test:prod": "vitest --no-watch",
    "version:patch": "pnpm build && npm version patch -m 'Bump version %s' && pnpm push:tags",
    "version:minor": "pnpm build && npm version minor -m 'Bump version %s' && pnpm push:tags",
    "version:major": "pnpm build && npm version major -m 'Bump version %s' && pnpm push:tags",
    "push:tags": "git push origin $(git rev-parse --abbrev-ref HEAD) --follow-tags",
    "codegen": "graphql-codegen -r dotenv/config -c codegen/codegen.ts"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
